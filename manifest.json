{
    "manifest_version": 2,
  
    "name": "SiteBlocker",
    "version": "1.0.0",
    "description": "Block non-important info from your browsers",
  
    "content_scripts": [{
      "js": ["content.js"],
      "matches": ["http://*/*", "https://*/*"]
    }],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
      }
  }