{
    "manifest_version": 2,
    "name": "Simple Blocker",
    "version": "1.0.0",
    "description": "Block distracting websites",
  
    "content_scripts": [{
      "js": ["content.js"],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_end"
    }],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
      }
  }